{
  "name": "@davidsneighbour/hugo-youtube",
  "description": "A better YouTube shortcode (and partial) for GoHugo",
  "version": "1.2023.0",
  "license": "MIT",
  "private": true,
  "author": "Patrick Kollitsch <patrick@davids-neighbour.com>",
  "bugs": {
    "url": "https://github.com/davidsneighbour/hugo-youtube/issues"
  },
  "homepage": "https://github.com/davidsneighbour/hugo-youtube",
  "dependencies": {
    "@davidsneighbour/configurations": "^5.2.13"
  },
  "scripts": {
    "release": "commit-and-tag-version --release-as patch -a -t \"modules/youtube/v\" --releaseCommitMessageFormat \"chore(release): v{{currentTag}}\" && ./bin/release/postrelease",
